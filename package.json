{
  "name": "notion-tweaks",
  "version": "1.0.0",
  "description": "This is a Chrome extension for notion",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development webpack --config=webpack.dev.js --mode=development",
    "build": "webpack --config webpack.prod.js"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.3.0",
    "terser-webpack-plugin": "^5.2.4",
    "ts-loader": "^9.2.5",
    "webpack-cli": "^4.8.0",
    "webpack-merge": "^5.8.0",
    "ws": "^8.2.2",
    "zip-webpack-plugin": "^4.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true,
      "webextensions": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browser": {
    "fs": false
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/eetann/notion-tweaks.git"
  },
  "author": "eetann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eetann/notion-tweaks/issues"
  },
  "homepage": "https://github.com/eetann/notion-tweaks#readme",
  "dependencies": {
    "@mui/material": "^5.0.2",
    "@notionhq/client": "^0.3.2",
    "@types/chrome": "^0.0.157",
    "dayjs": "^1.10.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.17.1",
    "react-hot-toast": "^2.1.1"
  }
}
