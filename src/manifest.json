{
  "manifest_version": 3,
  "name": "Notion Tweaks",
  "version": "1.0.0",
  "description": "For Notion",
  "author": "eetann",
  "host_permissions": [
    "https://script.google.com/",
    "https://script.googleusercontent.com/"
  ],
  "commands": {
    "today": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Open today page."
    }
  },
  "content_scripts": [
    {
      "matches": ["https://www.notion.so/*"],
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ],
  "background": {"service_worker": "background.js"}
}
